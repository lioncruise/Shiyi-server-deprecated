{
  "name": "shiyi-server",
  "version": "0.0.1",
  "main": "server/index.js",
  "description": "shiyi-server",
  "engines": {
    "node": "^0.12.*"
  },
  "scripts": {
    "start": "set DEBUG=*,-not_this;npm install && npm run doc && npm run fake && npm run server",
    "doc": "set DEBUG=*,-not_this;node --harmony ./doc/index.js",
    "server": "set DEBUG=*,-not_this;node --harmony worker.js",
    "dev": "set DEBUG=*,-not_this;./node_modules/.bin/node-dev --harmony ./worker.js",
    "init": "set DEBUG=*,-not_this;node --harmony ./init/index.js",
    "fake": "set DEBUG=*,-not_this;npm run init && node --harmony ./fake/index.js",
    "jshint": "set DEBUG=*,-not_this;./node_modules/.bin/jshint ."
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:GKuChan/shiyi-server.git"
  },
  "dependencies": {
    "cfork": "~1.3.1",
    "chance": "~0.7.7",
    "co": "~4.6.0",
    "co-fs-extra": "~1.1.0",
    "copy-to": "~2.0.1",
    "debug": "~2.2.0",
    "is-json": "~1.2.5",
    "koa": "~1.0.0",
    "koa-compose": "~2.3.0",
    "koa-formidable": "~1.0.0",
    "koa-logger": "~1.3.0",
    "koa-onerror": "~1.2.1",
    "koa-parameter": "~2.0.0",
    "koa-resource-router": "~0.4.0",
    "koa-router": "~5.1.2",
    "koa-rt": "~0.0.2",
    "koa-session": "~3.3.1",
    "koa-static-cache": "~3.1.2",
    "moment": "~2.10.6",
    "ms": "~0.7.1",
    "process": "~0.11.2",
    "sequelize": "~3.9.0",
    "sqlite3": "~3.1.0",
    "superagent": "~1.4.0",
    "urllib": "~2.4.0",
    "utility": "~1.4.0"
  },
  "devDependencies": {
    "autod": "~2.0.1",
    "co-mocha": "~1.1.2",
    "gulp": "~3.9.0",
    "istanbul": "~0.3.20",
    "jshint": "~2.8.0",
    "mm": "~1.3.4",
    "mocha": "~2.3.2",
    "node-dev": "~2.7.1",
    "should": "~7.1.0",
    "supertest": "~1.1.0"
  }
}
